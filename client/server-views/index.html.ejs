<!doctype html>
<html>
<head>
    <base href="/">

    <link href="/roboto-regular,roboto-medium,roboto-bold,roboto-italic/fonts.css" rel="stylesheet">

    <style>
        [ng-cloak] {
            display: none !important;
        }
    </style>
</head>
<body layout="column" layout-fill ng-cloak>

<div class="view-progress" ng-show="isViewLoadingSlow"
     layout="column" layout-fill>
  <md-progress-linear></md-progress-linear>
</div>
<ng-view layout="column" layout-fill layout-padding></ng-view>


<% if (env === 'production') { %>
    <script src="main.min.js"></script>
<% }  else { %>
    <script src="jspm_packages/system.js"></script>
    <script src="config.js"></script>
    <script>
        System.import('index');
    </script>
<% } %>
</body>
</html>
